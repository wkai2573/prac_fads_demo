<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  package="me.wkai.prac_fads_demo">

  <!--網路權限-->
  <uses-permission android:name="android.permission.INTERNET" /> <!--網路-->
  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!--網路資訊-->
  <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" /> <!--wifi資訊-->
  <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

  <application
    android:allowBackup="true"
    android:dataExtractionRules="@xml/data_extraction_rules"
    android:fullBackupContent="@xml/backup_rules"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.prac_fads_demo"
    tools:targetApi="31">
    <activity
      android:name="me.wkai.prac_fads_demo.MainActivity"
      android:exported="true"
      android:label="@string/app_name"
      android:theme="@style/Theme.prac_fads_demo">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>